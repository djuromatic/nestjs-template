{
  "name": "nestjs-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "engines": {
    "node": ">= 18.0.0 <= 18.15.0"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "./apps/*",
      "./libs/*"
    ]
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/nestjs-template/main",
    "lint": "eslint \"{src,apps,libs,test,utils}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nestjs-template/test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./apps/seeder/src/config/migration-data-source.ts",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./apps/seeder/src/migrations/$npm_config_name",
    "migration:generate": "npm run typeorm -- migration:generate ./apps/seeder/src/migrations/$npm_config_name",
    "migration:run": "npm run typeorm migration:run",
    "migration:test": "npm run typeorm schema:log",
    "migration:revert": "npm run typeorm migration:revert",
    "typeorm-seeder": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./apps/seeder/src/config/seeder-data-source.ts",
    "seeder:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./apps/seeder/src/seeders/$npm_config_name",
    "seeder:generate": "npm run typeorm -- migration:generate ./apps/seeder/src/seeders/$npm_config_name",
    "seeder:run": "npm run typeorm-seeder migration:run",
    "seeder:revert": "npm run typeorm-seeder migration:revert",
    "nestjs-template:cert": "./scripts/generate-cert.sh",
    "app:install": "yarn && yarn workspace @libs/modules install && yarn workspace @libs/utils install  && yarn workspace @app/nestjs-template install",
    "app:install:production": "yarn --production=true && cd ./libs/modules && yarn --production=true && cd ../utils && yarn --production=true",
    "libs:build:modules": "cd ./libs/modules && nest build",
    "libs:build:utils": "cd ./libs/utils && nest build"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.29.3",
    "express-jwt": "^8.4.1",
    "jwks-rsa": "^3.0.1",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@concepta/typeorm-seeding": "^4.0.0-beta.0",
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
