{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-template/src",
  "compilerOptions": {
    "plugins": ["@nestjs/swagger/plugin"],
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nestjs-template/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nestjs-template",
  "projects": {
    "nestjs-template": {
      "type": "application",
      "root": "apps/nestjs-template",
      "entryFile": "main",
      "sourceRoot": "apps/nestjs-template/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestjs-template/tsconfig.app.json"
      }
    },
    "@libs/modules": {
      "type": "library",
      "root": "libs/modules",
      "entryFile": "index",
      "sourceRoot": "libs/modules",
      "compilerOptions": {
        "tsConfigPath": "libs/modules/tsconfig.json"
      }
    },
    "@libs/utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.json"
      }
    },
    "seeder": {
      "type": "application",
      "root": "apps/seeder",
      "entryFile": "main",
      "sourceRoot": "apps/seeder/src",
      "compilerOptions": {
        "tsConfigPath": "apps/seeder/tsconfig.app.json"
      }
    }
  }
}
